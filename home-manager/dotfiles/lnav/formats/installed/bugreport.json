{
  "bugreport": {
    "$schema": "https://lnav.org/schemas/format-v1.schema.json",
    "title": "Android bugreport",
    "description": "bugreport generated by command 'adb bugreport'",
    "url": "https://developer.android.com/studio/debug/bug-report",
    "timestamp-format": [
      "%m-%d %H:%M:%S.%L"
    ],
    "regex": {
      "bugreport-logcat": {
        "pattern": "^(?<timestamp>\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}\\.\\d{3})\\d* *(?<uid>\\S+\\s*) *(?<pid>\\s?\\d+) *(?<tid>\\s?\\d+) *(?<level>.{1}) *(?<tag>\\S+\\s*): *(?<body>.*)"
      }
    },
    "level-field": "level",
    "level": {
      "fatal": "F",
      "error": "E",
      "warning": "W",
      "info": "I",
      "debug": "D",
      "trace": "V"
    },
    "value": {
      "level": {
        "kind": "string",
        "foreign-key": true
      }, 
      "tag" : {"kind":"string", "identifier": true},
      "uid" : {"kind":"string", "identifier":true},
      "pid" : {"kind":"integer", "identifier":true},
      "tid" : {"kind":"integer", "identifier":true},
      "body": {"kind":"string", "identifier": false}
    },
    "highlights": {
      "ae_tick_time":  {"pattern": "Average TICK time:.*", "underline": true},
      "ae_volume":  {"pattern": "Volume - .*dB", "underline": true}
    },
    "sample": [
      {
        "line": "11-11 06:11:19.036  root  3088  3088 I zygote  : option[46]=-Xfingerprint:VolvoCars/ihu_vcc/ihu_abl_car:9/PA1A.200309.002/ihu03191559:user/test-keys",
        "level": "info"
      },
      {
        "line": "11-11 06:11:19.039  2931  2385  2598 I AudioEngine: [CaptureDevice_allocCaptureDevice] :open pipe /dev/socket/audioengine_plugin/input/RadioStream_capSnd000  sndFdr 44",
        "level": "info"
      },
      {
        "line": "11-11 06:11:19.039111  2931  2385  2598 I AudioEngine: [CaptureDevice_allocCaptureDevice] :open pipe /dev/socket/audioengine_plugin/input/RadioStream_capSnd000  sndFdr 44",
        "level": "info"
      }
    ]
  }
}
