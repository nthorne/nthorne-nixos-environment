{
  "logcat": {
    "$schema": "https://lnav.org/schemas/format-v1.schema.json",
    "title": "Android logcat",
    "description": "logcat generated by command 'adb logcat'",
    "url": "https://developer.android.com/studio/command-line/logcat.html",
    "timestamp-format": [
      "%m-%d %H:%M:%S.%L"
    ],
    "regex": {
      "std-logcat": {
        "pattern": "^(?<timestamp>\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}\\.\\d{3}) *(?<pid>\\s?\\d+) *(?<tid>\\s?\\d+) *(?<level>.{1}) *(?<tag>\\S+\\s*): *(?<body>.*)"
      }
    },
    "level-field": "level",
    "level": {
      "fatal": "F",
      "error": "E",
      "warning": "W",
      "info": "I",
      "debug": "D",
      "trace": "V"
    },
    "value": {
      "level": {
        "kind": "string",
        "foreign-key": true
      }, 
      "tag" : {"kind":"string", "identifier": true},
      "pid" : {"kind":"integer", "identifier":true},
      "tid" : {"kind":"integer", "identifier":true},
      "body": {"kind":"string", "identifier": false}
    },
    "highlights": {
      "ae_tick_time":  {"pattern": "Average TICK time:.*", "underline": true},
      "ae_volume":  {"pattern": "Volume - .*dB", "underline": true}
    },
    "sample": [
      {
        "line": "10-13 15:39:27.517 2118 2118 I GoogleURLConnFactory   : Using platform SSLCertificateSocketFactory",
        "level": "info"
      }
    ]
  }
}
