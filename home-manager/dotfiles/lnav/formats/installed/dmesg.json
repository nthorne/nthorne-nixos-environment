{
  "dmesg": {
    "$schema": "https://lnav.org/schemas/format-v1.schema.json",
    "title": "dmesgs logs",
    "description": "logs generated by command 'dmesg'",
    "url": "https://developer.android.com/studio/command-line/logcat.html",
    "timestamp-format": [
      "%s.%f"
    ],
    "regex": {
      "std-dmesg": {
        "pattern": "^\\[[ \\t]*(?<timestamp>\\d+\\.\\d+)\\] *(?<body>.*)"
      }
    },
    "value": {
      "body": {"kind":"string", "identifier": false}
    },
    "highlights": {
      "intel sound driver":  {"pattern": "snd_.*", "underline": true},
      "sound driver":  {"pattern": "volvo_sid.*"},
      "DebugFS": {"pattern": "Debugfs.*"},
      "init actions": {"pattern": "init: processing action .*"}
    },
    "sample": [
      {
        "line": "[   1.123456] This is a sample line"
      }
    ]
  }
}
